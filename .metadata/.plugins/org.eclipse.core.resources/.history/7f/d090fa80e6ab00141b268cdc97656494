import java.awt. *;
import java.awt.geom.Ellipse2D;

import javax.swing. *;



public class BullseyeComponent extends JApplet {
	public void paint(Graphics g){
	
	Graphics2D g1 = (Graphics2D) g;
	Ellipse2D.Double circle = new Ellipse2D.Double(100,100,250,250);
	g1.draw(circle);
	g1.setColor(Color.BLACK);
	g1.fill(circle);	
	Ellipse2D.Double circle2 = new Ellipse2D.Double(100,100,25,25);
	g1.setColor(Color.green);
	g1.draw(circle2);
	g1.fill(circle2);

}
	
}
