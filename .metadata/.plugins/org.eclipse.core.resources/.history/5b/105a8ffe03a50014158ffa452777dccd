
public class Roachtester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Roachpop roach = new Roachpop(10);
		roach.waitForDouble();
		roach.spray();
		System.out.println(roach.getRoach());
		roach.waitForDouble();
		roach.spray();
		System.out.println(roach.getRoach());
		roach.waitForDouble();
		roach.spray();
		System.out.println(roach.getRoach());

	}

}
