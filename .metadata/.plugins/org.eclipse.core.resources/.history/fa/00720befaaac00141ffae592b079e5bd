import java.awt. *;

import javax.swing.*;

public class Housecomp extends JApplet {
	public void paint(Graphics g){
		
		Graphics2D g1 = (Graphics2D) g;
		Rectangle body = new Rectangle(400,400,400,400);
		Rectangle door = new Rectangle(450,650,70,150);
		Rectangle glass = new Rectangle(600,610,55,55);
		g1.draw(body);
		g1.draw(door);
		g1.draw(glass);
		
		
		
}
	
}
