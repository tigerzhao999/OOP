
public class Aeiou {
	public static void main(String[] args) {
		System.out.println("enter a string");
		Scanner scan = new Scanner(System.in);
		String user = scan.nextLine();
		
		
		
		
	}
	int counter = 0;
	
	
	String vowels = "aeiouAEIOU";
	
	for(int i = 0; i < user.length(); i++){
		c = " " + user.charAt(i);
		if(vowels.contains(c)){
			counter++;
		}
		
	}
	

}
